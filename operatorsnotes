let b = 5
console.log(a/b)
console.log(a%b)

let a = 10
let b = 5
console.log(a**b)
a++
b--
console.log(a)
console.log(b)

var num = 40
num+=10
num-=20
console.log(num)

var num1 = 25
var num2 = 30
var con = num1<num2 && num1==25
var con = num1<num2 || num1==20
var con = (!(false))
console.log(con)
console.log(num1==num2)
console.log(num1!=num2)
